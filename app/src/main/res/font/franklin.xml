<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!-- regular -->
    <font
        android:font="@font/librefranklin_regular"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/librefranklin_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!-- italic -->
    <font
        android:font="@font/librefranklin_italic"
        android:fontStyle="italic"
        android:fontWeight="400"

        app:font="@font/librefranklin_italic"
        app:fontStyle="italic"
        app:fontWeight="400" />

    <!-- bold -->
    <font
        android:font="@font/librefranklin_bold"
        android:fontStyle="normal"
        android:fontWeight="700"

        app:font="@font/librefranklin_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />

    <!-- semi -->
    <font
        android:font="@font/librefranklin_semibold"
        android:fontStyle="normal"
        android:fontWeight="700"

        app:font="@font/librefranklin_semibold"
        app:fontStyle="normal"
        app:fontWeight="700" />

    <!-- black-->
    <font
        android:font="@font/librefranklin_black"
        android:fontStyle="normal"
        android:fontWeight="700"

        app:font="@font/librefranklin_black"
        app:fontStyle="normal"
        app:fontWeight="700" />


</font-family>